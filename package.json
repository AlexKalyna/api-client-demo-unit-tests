{
  "name": "api-client-demo-unit-tests",
  "version": "1.0.0",
  "description": "Demo project showcasing unit testing skills with Vitest, Axios, retry logic, and circuit breaker patterns",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "type-check": "tsc --noEmit",
    "dev": "vitest --watch"
  },
  "keywords": [
    "vitest",
    "axios",
    "unit-testing",
    "typescript",
    "retry-logic",
    "circuit-breaker",
    "async-testing",
    "mocking"
  ],
  "author": "Senior Automation QA Engineer",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "@vitest/coverage-v8": "^1.0.0",
    "@vitest/ui": "^1.0.0",
    "eslint": "^8.54.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  },
  "dependencies": {
    "axios": "^1.6.0"
  }
}
